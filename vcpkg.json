{
    "name": "dwarf-engine",
    "version-string": "0.0.1",
    "supports": "(windows & x64 & static) | (linux & x64) | (osx & x64)",
    "dependencies": [
        "glad",
        "glfw3",
        "glm",
        {
            "name": "imgui",
            "default-features": false,
            "features": [
