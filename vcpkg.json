{
    "name": "dwarf-engine",
    "version-string": "0.0.1",
    "supports": "(windows & x64 & static) | (linux & x64) | (osx & x64 & static)",
    "dependencies": [
        "glad",
        {
            "name": "glfw3",
            "default-features": false,
            "platform": "(windows & x64 & static) | (linux & x64) | (osx & x64 & static)"
        },
        "glm",
        {
            "name": "imgui",
            "default-features": false,
            "features": [
                "glfw-binding",
                "opengl3-binding",
                "docking-experimental"
            ]
        },
        {
            "name": "assimp",
            "default-features": false,
            "platform": "(windows & x64 & static) | (linux & x64) | (osx & x64 & static)"
        },
        "stb",
        {
            "name": "nlohmann-json",
            "default-features": false,
            "platform": "(windows & x64 & static) | (linux & x64) | (osx & x64 & static)"
        },
        {
            "name": "nativefiledialog",
            "default-features": false,
            "platform": "(windows & x64 & static) | (linux & x64) | (osx & x64 & static)"
        },
        {
            "name": "bext-di",
            "default-features": false,
            "platform": "(windows & x64 & static) | (linux & x64) | (osx & x64 & static)"
        }
    ]
}